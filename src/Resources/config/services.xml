<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="ShopBite\Checkout\Cart\LineItemFactoryHandler\ContainerLineItemFactory">
            <tag name="shopware.cart.line_item.factory"/>
        </service>

        <service id="ShopBite\Config\SalesChannel\ShopBiteConfigRoute">
            <argument key="$systemConfigService" type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <tag name="controller.service_arguments"/>
        </service>

        <service id="ShopBite\BusinessHour\SalesChannel\BusinessHourRoute">
            <argument type="service" id="shopbite_business_hour.repository"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="ShopBite\Holiday\SalesChannel\HolidayRoute">
            <argument type="service" id="shopbite_holiday.repository"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="ShopBite\Service\CustomFieldsInstaller">
            <argument key="$customFieldSetRepository" type="service" id="custom_field_set.repository"/>
            <argument key="$customFieldSetRelationRepository" type="service" id="custom_field_set_relation.repository"/>
        </service>

        <service id="ShopBite\Checkout\Cart\ReceiptPrintTypeProcessor">
            <argument type="service" id="product.repository"/>
            <tag name="shopware.cart.processor" priority="4500"/>
            <tag name="shopware.cart.collector" priority="4500"/>
        </service>

        <!-- DAL entity definitions -->
        <service id="ShopBite\Holiday\HolidayDefinition">
            <tag name="shopware.entity.definition"/>
        </service>

        <service id="ShopBite\BusinessHour\BusinessHourDefinition">
            <tag name="shopware.entity.definition"/>
        </service>

        <!-- Entity extension(s) -->
        <service id="ShopBite\Extension\SalesChannel\SalesChannelExtension">
            <tag name="shopware.entity.extension"/>
        </service>

    </services>
</container>