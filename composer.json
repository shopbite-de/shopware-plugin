{
    "name": "shopbite-de/shopware-plugin",
    "description": "Shopware 6 Plugin for ShopBite Storefront",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ShopBite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shopbite\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Lirim Veliu",
            "email": "lirim@veliu.net"
        }
    ],
    "extra": {
        "shopware-plugin-class": "ShopBitePlugin",
        "label": {
            "de-DE": "Holabe Core Plugin",
            "en-GB": "Holabe Core Plugin"
        }
    },
    "require": {
        "php": "^8.4",
        "shopware/core": "~6.7.0"
    },

    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    }
}
